{{log model.users}}


{{#each model.user as |model|}}

<section>
    <div class="container">
        <div class = "row">
            <h1 class = "bg-txt">Speaker profile</h1>

{{#link-to "users.show" model.id}}  <h2 class = "md-txt">{{model.firstName}} <br>{{model.lastName}}</h2> {{/link-to}}
